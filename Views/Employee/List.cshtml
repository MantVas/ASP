@{ ViewData["Title"] = "List Page"; }
@model List<Employee>
<div class="text-center">
    @foreach (var item in Model)
    {
        <p>@item.Id @item.Name <a href="@Url.Action("Delete", "Employee", new { ID = item.Id })">Delete</a></p>
    }
    <hr />
    <form method="post" asp-controller="Employee" asp-action="Create">
        <label>Name:</label>
        <input type="text" name="name" />
        <input type="submit" value="Create Employee" name="submit" />
    </form>


